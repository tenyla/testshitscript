local url = "https://raw.githubusercontent.com/TheGreatSageThatEqualToHeaven/InsideBase/refs/heads/main/antikick"
local success, err = pcall(function()
    loadstring(game:HttpGet(url))()
end)
if not success then
    warn("Failed to run anti-kick script: "..err)
else
    print("Anti-kick script loaded successfully.")
end

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "DraggableGUI"
screenGui.ResetOnSpawn = false
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 200, 0, 100)
frame.Position = UDim2.new(0.5, -100, 0.5, -50)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.BorderSizePixel = 0
frame.Parent = screenGui
frame.Active = true
frame.Draggable = true

local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 25)
title.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
title.TextColor3 = Color3.new(1,1,1)
title.Text = "Draggable GUI"
title.Parent = frame

local button1 = Instance.new("TextButton")
button1.Size = UDim2.new(0.8, 0, 0, 30)
button1.Position = UDim2.new(0.1, 0, 0.4, 0)
button1.Text = "Tp inside base"
button1.BackgroundColor3 = Color3.fromRGB(70, 130, 180)
button1.TextColor3 = Color3.new(1,1,1)
button1.Parent = frame

local button2 = Instance.new("TextButton")
button2.Size = UDim2.new(0.8, 0, 0, 30)
button2.Position = UDim2.new(0.1, 0, 0.75, 0)
button2.Text = "TpToBase"
button2.BackgroundColor3 = Color3.fromRGB(70, 130, 180)
button2.TextColor3 = Color3.new(1,1,1)
button2.Parent = frame

button1.MouseButton1Click:Connect(function()
    local url = "https://raw.githubusercontent.com/TheGreatSageThatEqualToHeaven/InsideBase/refs/heads/main/sab"
    local success, err = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if not success then
        warn("Failed to run script from Tp inside base button: "..err)
    end
end)

button2.MouseButton1Click:Connect(function()
    local url = "https://raw.githubusercontent.com/TheGreatSageThatEqualToHeaven/InsideBase/refs/heads/main/sabtptobase"
    local success, err = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if not success then
        warn("Failed to run script from TpToBase button: "..err)
    end
end)
